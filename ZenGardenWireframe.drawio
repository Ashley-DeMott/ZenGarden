          <mxGeometry x="177" y="240" width="260" height="25" as="geometry" />
        </mxCell>
        <mxCell id="uqW2ANn6eiQzJRIduRhj-38" value="&lt;span style=&quot;color: rgb(0, 0, 0); font-family: Helvetica; font-size: 12px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: 400; letter-spacing: normal; orphans: 2; text-align: left; text-indent: 0px; text-transform: none; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; background-color: rgb(251, 251, 251); text-decoration-thickness: initial; text-decoration-style: initial; text-decoration-color: initial; float: none; display: inline !important;&quot;&gt;A demonstration of what can be accomplished through CSS-based design. Select any style sheet from the list to load it into this page.&lt;/span&gt;" style="text;whiteSpace=wrap;html=1;" vertex="1" parent="1">
